{
  "name": "kong-auth-elements",
  "version": "0.1.0",
  "private": true,
  "main": "kong-auth-elements.umd.min.js",
  "scripts": {
    "serve": "echo \"\nRun 'yarn serve:components' to use Vue Dev Tools.\n(may require additional imports in /dev/serve-components/ComponentsApp.vue)\n** This will not allow testing embedded styles and other Custom Element features. **\n\n-- OR --\n\nRun 'yarn serve:elements' to view HTML element usage.\n(may require additional imports in /dev/serve-components/index.ts)\n\n\"",
    "serve:components": "vue-cli-service serve ./dev/serve-components",
    "serve:elements": "vue-cli-service serve ./dev/serve-elements",
    "demo": "yarn build && rm -rf ./demo/dist-copy && cp -R ./dist ./demo/dist-copy && echo \"\nNote: The demo does NOT support hot reload.\" && serve ./demo",
    "build": "vue-cli-service build --target lib --name kong-auth-elements --inline-vue ./src/index.ts",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "spec:download": "curl http://localhost:8080/api/swagger/doc.json > spec.json",
    "generate:api-client": "yarn openapi-generator-cli generate --skip-validate-spec -i spec.json -g typescript-axios -o src/services/kauth-api-client -p modelPropertyNaming=original"
  },
  "dependencies": {
    "@kongponents/kalert": "^6.5.9",
    "@kongponents/kbutton": "^6.5.9",
    "@kongponents/kcard": "^6.5.9",
    "@kongponents/kcheckbox": "^6.5.9",
    "@kongponents/kicon": "^6.5.9",
    "@kongponents/kinput": "^6.5.9",
    "@kongponents/klabel": "^6.5.9",
    "@kongponents/kooltip": "^6.5.9",
    "@kongponents/kskeleton": "^6.5.9",
    "@kongponents/styles": "^6.5.9",
    "@openapitools/openapi-generator-cli": "^2.4.18",
    "@segment/analytics.js-core": "^4.1.11",
    "@vue/compat": "^3.2.22",
    "@xstate/vue": "^0.8.1",
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
    "swrv": "^0.9.4-2",
    "vue": "^3.0.0",
    "xstate": "^4.26.1"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^24.0.19",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "axe-core": "^4.3.5",
    "cypress": "^9.1.0",
    "cypress-axe": "^0.13.0",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "faker": "^5.5.3",
    "jest-junit": "^13.0.0",
    "postcss-custom-properties": "^8.0.10",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0"
  },
  "files": [
    "./dist/*"
  ]
}
