{
  "name": "@kong/kong-auth-elements",
  "version": "0.0.0-development",
  "type": "module",
  "main": "dist/kong-auth-elements.vue.umd.js",
  "module": "dist/kong-auth-elements.vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/kong-auth-elements.vue.es.js",
      "require": "./dist/kong-auth-elements.vue.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "commit": "cz",
    "build": "yarn stylelint && yarn lint && yarn typecheck && yarn build:package && yarn build:package:omitVue && vue-tsc --emitDeclarationOnly",
    "build:package": "cross-env INCLUDE_VUE=yes vite build",
    "build:package:omitVue": "cross-env INCLUDE_VUE=no vite build",
    "build:visualize": "cross-env BUILD_VISUALIZER=true INCLUDE_VUE=no vite build -m production",
    "serve:components": "cross-env SERVE_MODE=components vite",
    "serve:elements": "cross-env SERVE_MODE=elements vite",
    "serve:demo": "rimraf ./demo/dist-copy && yarn build && cp -R ./dist ./demo/dist-copy && echo \"\n    \\x1b[30m\\x1b[103m Note: The demo does NOT support hot module reload \\x1b[0m\" && yarn serve",
    "serve": "serve -p 3333 ./demo",
    "serve:help": "echo \"\nRun 'yarn serve:components' to import components and use Vue Dev Tools.\n(may require additional imports in /dev/serve-components/ComponentsApp.vue)\n\\x1b[33m* This will not allow testing embedded styles and other Custom Element features. *\\x1b[0m\n\n-- OR --\n\nRun 'yarn serve:elements' to import native Web Components.\n(may require additional imports in /dev/serve-elements/index.ts)\n\\x1b[33m* This will not allow you to utilize Vue Dev Tools in the browser (custom elements are not currently supported). *\\x1b[0m\n\n\"",
    "test": "cypress run --component -b chrome",
    "test:open": "cypress open --component -b chrome",
    "test:spec": "cypress run --component -b chrome --spec",
    "lint": "eslint '**/*.{js,ts,vue}'",
    "lint:fix": "eslint '**/*.{js,ts,vue}' --fix",
    "typecheck": "vue-tsc --noEmit",
    "semantic-release": "semantic-release",
    "stylelint": "stylelint './src/**/*.{css,scss,sass,less,styl,vue}'",
    "stylelint:fix": "stylelint './src/**/*.{css,scss,sass,less,styl,vue}' --fix"
  },
  "dependencies": {
    "@kong/kauth-client-typescript-axios": "^0.1858.0",
    "@xstate/vue": "^0.8.1",
    "axios": "^0.27.2",
    "vue": "^3.2.47",
    "vue-recaptcha": "^2.0.3",
    "xstate": "^4.37.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/types": "^7.21.2",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@cypress/vite-dev-server": "^5.0.4",
    "@cypress/vue": "^5.0.4",
    "@digitalroute/cz-conventional-changelog-for-jira": "^7.5.1",
    "@kong/kongponents": "^8.37.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^16.18.15",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.36.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss-custom-properties": "^10.0.0",
    "postcss-html": "^1.5.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.59.2",
    "semantic-release": "^19.0.5",
    "serve": "^14.2.0",
    "start-server-and-test": "^1.15.4",
    "stylelint": "^14.16.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "typescript": "~4.9.5",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0",
    "zxcvbn": "^4.4.2"
  },
  "peerDependencies": {
    "@kong/kongponents": "^8.37.0",
    "axios": "^0.27.2",
    "vue": "^3.2.47"
  },
  "files": [
    "dist"
  ],
  "typings": "dist/types",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kong/kong-auth-elements.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "license": "UNLICENSED",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "jiraOptional": true,
      "jiraLocation": "post-description",
      "jiraPrepend": "[",
      "jiraAppend": "]"
    }
  },
  "engines": {
    "node": ">=16.19.0"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.17"
  }
}
