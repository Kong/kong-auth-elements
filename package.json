{
  "name": "@kong/kong-auth-elements",
  "version": "0.0.0-development",
  "main": "dist/kong-auth-elements.umd.min.js",
  "scripts": {
    "serve:components": "vue-cli-service serve ./dev/serve-components",
    "serve:elements": "vue-cli-service serve ./dev/serve-elements",
    "serve": "yarn serve:help",
    "serve:help": "echo \"\nRun 'yarn serve:components' to import components and use Vue Dev Tools.\n(may require additional imports in /dev/serve-components/ComponentsApp.vue)\n\\x1b[33m* This will not allow testing embedded styles and other Custom Element features. *\\x1b[0m\n\n-- OR --\n\nRun 'yarn serve:elements' to import HTML Custom Elements.\n(may require additional imports in /dev/serve-elements/index.ts)\n\\x1b[33m* This will not allow you to utilize Vue Dev Tools in the browser (custom elements are not currently supported). *\\x1b[0m\n\n\"",
    "build": "vue-cli-service build --target lib --name kong-auth-elements --inline-vue ./src/index.ts && rm ./dist/demo.html && yarn build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "cypress:open": "yarn test:open",
    "lint": "vue-cli-service lint",
    "generate:api-client": "yarn openapi-generator-cli generate --skip-validate-spec -i src/services/kauth-api-client/v1/spec.json -g typescript-axios -o src/services/kauth-api-client/v1/api -p modelPropertyNaming=original",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@kongponents/kalert": "^6.7.11",
    "@kongponents/kbutton": "^6.7.10",
    "@kongponents/kcheckbox": "^6.7.11",
    "@kongponents/kicon": "^6.7.7",
    "@kongponents/kinput": "^6.7.11",
    "@kongponents/klabel": "^6.7.11",
    "@kongponents/kooltip": "^6.7.7",
    "@kongponents/kskeleton": "^6.7.11",
    "@kongponents/styles": "^6.7.7",
    "@openapitools/openapi-generator-cli": "^2.4.25",
    "@vue/compat": "^3.2.29",
    "@xstate/vue": "^0.8.1",
    "axios": "^0.25.0",
    "core-js": "^3.6.5",
    "vue": "^3.2.29",
    "xstate": "^4.26.1"
  },
  "devDependencies": {
    "@cypress/vue": "^3.1.0",
    "@cypress/webpack-dev-server": "^1.8.0",
    "@types/faker": "^5.5.9",
    "@types/node": "^16.11.21",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "3.2.29",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "axe-core": "^4.3.5",
    "cypress": "^9.3.1",
    "cypress-axe": "^0.14.0",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "faker": "^5.5.3",
    "html-webpack-plugin": "4",
    "node-sass": "^4.11.0",
    "postcss-custom-properties": "^10.0.0",
    "sass-loader": "^8.0.2",
    "semantic-release": "^18.0.1",
    "typescript": "~4.1.5",
    "vue-tsc": "^0.31.1"
  },
  "peerDependencies": {
    "axios": "^0.25.0"
  },
  "files": [
    "dist/**/*.{md,js,map,d.ts}"
  ],
  "typings": "./dist/types",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kong/kong-auth-elements.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "license": "UNLICENSED",
  "release": {
    "branches": [
      "main"
    ]
  },
  "engines": {
    "node": "<=14.18 <16"
  },
  "volta": {
    "node": "14.18.2",
    "yarn": "1.22.17"
  }
}
