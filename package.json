{
  "name": "@kong/kong-auth-elements",
  "version": "0.0.0-development",
  "main": "dist/kong-auth-elements.umd.js",
  "module": "dist/kong-auth-elements.es.js",
  "exports": {
    ".": {
      "import": "./dist/kong-auth-elements.es.js",
      "require": "./dist/kong-auth-elements.umd.js"
    }
  },
  "scripts": {
    "commit": "cz",
    "build": "vue-tsc --noEmit && vite build && vue-tsc --emitDeclarationOnly",
    "serve:components": "cross-env SERVE_MODE=components vite",
    "serve:elements": "cross-env SERVE_MODE=elements vite",
    "serve:demo": "rm -rf ./demo/dist-copy && yarn build && cp -R ./dist ./demo/dist-copy && echo \"\n    \\x1b[30m\\x1b[103m Note: The demo does NOT support hot module reload \\x1b[0m\" && serve -p 3333 ./demo",
    "serve": "yarn serve:help",
    "serve:help": "echo \"\nRun 'yarn serve:components' to import components and use Vue Dev Tools.\n(may require additional imports in /dev/serve-components/ComponentsApp.vue)\n\\x1b[33m* This will not allow testing embedded styles and other Custom Element features. *\\x1b[0m\n\n-- OR --\n\nRun 'yarn serve:elements' to import native Web Components.\n(may require additional imports in /dev/serve-elements/index.ts)\n\\x1b[33m* This will not allow you to utilize Vue Dev Tools in the browser (custom elements are not currently supported). *\\x1b[0m\n\n\"",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "lint": "eslint '**/*.{js,ts,vue}'",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@kong/kauth-client-typescript-axios": "^0.891.0",
    "@xstate/vue": "^0.8.1",
    "axios": "^0.26.1",
    "vue": "^3.2.37",
    "xstate": "^4.32.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/types": "^7.18.4",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@cypress/vite-dev-server": "^2.2.3",
    "@cypress/vue": "^3.1.2",
    "@digitalroute/cz-conventional-changelog-for-jira": "^7.3.0",
    "@kong/kongponents": "^7.0.0-beta.44",
    "@rushstack/eslint-patch": "^1.1.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^16.11.39",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.17.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss-custom-properties": "^10.0.0",
    "sass": "^1.52.2",
    "semantic-release": "^19.0.3",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.3",
    "vite": "^2.9.12",
    "vue-tsc": "^0.35.2",
    "zxcvbn": "^4.4.2"
  },
  "peerDependencies": {
    "axios": "^0.26.1"
  },
  "files": [
    "dist"
  ],
  "typings": "dist/types",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kong/kong-auth-elements.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "license": "UNLICENSED",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "jiraOptional": true,
      "jiraLocation": "post-description",
      "jiraPrepend": "[",
      "jiraAppend": "]"
    }
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.17"
  }
}
